{
  "name": "ctrl-vk",
  "version": "0.0.0",
  "manifest_version": 2,
  "description": "Chrome extension for ctrl+v insertion of images to vk.com",
  "background" : {
    "page": "background.html"
  },
  "content_scripts": [{
        "all_frames": true,
        "matches": ["http://*/*","https://*/*"],
        "js": ["javascripts/jquery-1.8.2.min.js", "javascripts/app.js"]
  }],
  "permissions" : [
    "tabs",
    "notifications",
    "http://*/*",
    "https://*/*"
  ],
  "minimum_chrome_version" : "10.0.0.0"
}
