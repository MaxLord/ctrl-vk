{
  "name": "ctrl-vk",
  "version": "0.0.0",
  "manifest_version": 2,
  "description": "Chrome extension for ctrl+v insertion of images to vk.com",
  "content_scripts": [{
        "all_frames": true,
        "matches": ["http://*/*","https://*/*"],
        "js": ["javascripts/app.user.js"],
        "run_at": "document_end"
  }]
}
