{
  "name": "ctrl-vk",
  "version": "0.0.0",
  "manifest_version": 2,
  "description": "Chrome extension for ctrl+v insertion of images to vk.com",
  "background" : {
    "page": "background.html"
  },
  "permissions" : [
    "tabs",
    "notifications",
    "http://*/*",
    "https://*/*"
  ],
  "minimum_chrome_version" : "10.0.0.0"
}
