    {
      "name": "ctrl-vk",
      "version": "0.1.3",
      "manifest_version": 2,
      "description": "Chrome extension for ctrl+v insertion of images to vk.com",

      "content_scripts": [{
        "matches": [
            "http://*/*",
            "https://*/*"
        ],
        "js": ["jquery-1.9.1.min.js"
        ],
        "run_at": "document_end"
      }],

      "web_accessible_resources": [
        "jquery-1.9.1.min.js"
      ],

      "permissions" : [
        "tabs",
        "http://*/*",
        "https://*/*"
      ],

      "background": {
        "persistent": false,
        "scripts": ["background.js"]
      }
    }
