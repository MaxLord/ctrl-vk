{
  "name": "ctrl-vk",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Chrome extension for ctrl+v insertion of images to vk.com",

  "content_scripts": [{
    "matches": ["http://*/*","https://*/*"],
    "js": ["javascripts/jquery-1.9.1.min.js",
      "javascripts/app.user.js"
    ],
    "run_at": "document_end"
  }],

  "web_accessible_resources": [
    "javascripts/jquery-1.9.1.min.js",
    "javascripts/app.user.js"
  ]
}
